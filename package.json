    "bulma": "0.9.1",
    "bulmaswatch": "0.8.1",
    "moment": "2.29.4",
    "node-sass": "4.14.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-scripts": "4.0.0",
    "recharts": "1.8.5",
