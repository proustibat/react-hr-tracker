    "bulma": "0.9.0",
    "bulmaswatch": "0.8.1",
    "moment": "2.27.0",
    "node-sass": "4.14.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-scripts": "3.4.1",
    "recharts": "1.8.5",
    "typescript": "3.9.6"
