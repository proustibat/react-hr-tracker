    "@types/web-bluetooth": "0.0.9",
    "cz-conventional-changelog": "3.3.0",
    "cz-emoji": "1.3.1",
    "husky": "4.3.0",
    "prettier": "2.1.2",
    "prettier-eslint": "11.0.0",
    "prettier-eslint-cli": "5.0.0",
    "pretty-quick": "3.1.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  },
  "husky": {
